male(raja,neel,vij,luv,kush).
female(rani,priya,ruh,rita,mona).
father(neel,luv).
father(neel,rita).
father(vij,kush).
father(vij,mona).
father(raja,neel).
father(raja,vij).
mother(priya,luv).
mother(priya,rita).
mother(ruh,kush).
mother(ruh,mona).
mother(rani,neel).
mother(rani,vij).

sister(X,Y):-female(X),mother(M,X),mother(M,Y).
sister(X,Y):-male(X),mother(M,X),mother(M,Y).
